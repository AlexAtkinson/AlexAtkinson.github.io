{
  "headers": {
    "/service-worker.js": { "cache-control": "no-cache, no-store, must-revalidate" },
    "/index.html": { "cache-control": "public, max-age=0, must-revalidate" },
    "/assets/gists.json": { "cache-control": "public, max-age=60" },
    "/assets/theme.css": { "cache-control": "public, max-age=31536000, immutable" },
    "/assets/theme.js": { "cache-control": "public, max-age=31536000, immutable" },
    "/assets/images/optimized/*": { "cache-control": "public, max-age=31536000, immutable" },
    "/assets/images/*": { "cache-control": "public, max-age=604800" }
  }
}
